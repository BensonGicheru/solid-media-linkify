{
    "compilerOptions": {
      "strict": true,
      "noImplicitAny": false,
      "target": "es2020",
      "module": "ESNext",
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "jsx": "preserve",
      "jsxImportSource": "solid-js",
      "allowJs": true,
      "strictNullChecks": false,
      "resolveJsonModule": true,
      "noEmit": true,
    },
  }